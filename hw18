print("Введите  количество билетов")
ticket_num: int = int(input())

print("укажите возраст")
age: int = int(input())
price = int


# print(type(age), type(ticket_num))

def ticket_price(age):
    if age in range(0, 17):
        var = price == 0
    elif age in range(18, 25):
        var = price == 990*ticket_num
    else:
        var = price == 1300*ticket_num


print("price is", ticket_price(age))
